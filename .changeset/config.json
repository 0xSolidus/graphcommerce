{
  "$schema": "https://unpkg.com/@changesets/config@2.2.0/schema.json",
  "changelog": ["@changesets/changelog-github", { "repo": "graphcommerce-org/graphcommerce" }],
  "commit": false,
  "fixed": [
    ["@graphcommerce/magento*", "@graphcommerce/magento-mollie-payment"],
    [
      "@graphcommerce/cli",
      "@graphcommerce/ecommerce-ui",
      "@graphcommerce/framer-next-pages",
      "@graphcommerce/framer-scroller",
      "@graphcommerce/framer-utils",
      "@graphcommerce/graphql",
      "@graphcommerce/graphql-mesh",
      "@graphcommerce/image",
      "@graphcommerce/lingui-next",
      "@graphcommerce/next-ui",
      "@graphcommerce/react-hook-form",
      "@graphcommerce/next-confg"
    ]
  ],
  "access": "public",
  "baseBranch": "main",
  "updateInternalDependencies": "patch",
  "snapshot": {
    "useCalculatedVersion": true,
    "prereleaseTemplate": "{tag}-{datetime}-{commit}"
  }
}
