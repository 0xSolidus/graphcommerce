{
  "name": "@graphcommerce/magento-graphcms",
  "version": "2.125.3",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "yarn concurrently -k 'yarn:codegen -w' 'yarn:next dev'",
    "gen": "yarn mesh build --require dotenv/config && NEXT_PUBLIC_GRAPHQL_ENDPOINT=.mesh/schema.graphql yarn codegen",
    "build": "yarn gen && yarn next build && yarn next-sitemap",
    "start": "next start",
    "tsc:lint": "yarn tsc --noEmit -p .",
    "codegen": "NODE_TLS_REJECT_UNAUTHORIZED=0 CHOKIDAR_USEPOLLING=0 node -r dotenv/config node_modules/.bin/graphql-codegen -c codegen.mono.yml",
    "prepack": "concurrently yarn:prepack:*",
    "prepack:1": "yarn workspace @graphcommerce/next-config prepack",
    "prepack:2": "yarn workspace @graphcommerce/graphql-codegen-near-operation-file prepack",
    "prepack:3": "yarn workspace @graphcommerce/graphql-codegen-relay-optimizer-plugin prepack",
    "postinstall": "patch-package && patch-typed-document-node && yarn prepack"
  },
  "dependencies": {
    "@adobe/apollo-link-mutation-queue": "^1.0.2",
    "@apollo/client": "^3.4.16",
    "@graphcommerce/browserslist-config-pwa": "^3.0.2",
    "@graphcommerce/eslint-config-pwa": "^3.1.0",
    "@graphcommerce/framer-next-pages": "^2.106.11",
    "@graphcommerce/framer-scroller": "^0.3.2",
    "@graphcommerce/googletagmanager": "^0.2.9",
