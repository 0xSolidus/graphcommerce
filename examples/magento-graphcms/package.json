    "@graphcommerce/googletagmanager": "2.0.8",
    "@graphcommerce/graphcms-ui": "3.0.24",
    "@graphcommerce/graphql": "3.2.1",
    "@graphcommerce/graphql-mesh": "4.1.4",
    "@graphcommerce/image": "3.1.7",
