    "@graphcommerce/magento-graphql": "^2.103.9",
    "@graphcommerce/magento-newsletter": "^0.2.35",
    "@graphcommerce/magento-payment-braintree": "^2.104.39",
    "@graphcommerce/magento-payment-included": "^2.103.38",
    "@graphcommerce/magento-product": "^3.2.5",
    "@graphcommerce/magento-product-bundle": "^3.0.36",
    "@graphcommerce/magento-product-configurable": "^3.1.7",
    "@graphcommerce/magento-product-downloadable": "^3.0.36",
