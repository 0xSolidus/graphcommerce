    "@graphcommerce/graphql": "^2.104.0",
    "@graphcommerce/graphql-codegen-near-operation-file": "^2.103.8",
    "@graphcommerce/graphql-codegen-relay-optimizer-plugin": "^2.102.8",
    "@graphcommerce/graphql-mesh": "^3.2.0",
    "@graphcommerce/image": "^2.104.12",
    "@graphcommerce/magento-cart": "^3.2.5",
    "@graphcommerce/magento-cart-billing-address": "^2.105.2",
    "@graphcommerce/magento-cart-checkout": "^2.105.2",
    "@graphcommerce/magento-cart-coupon": "^2.105.34",
    "@graphcommerce/magento-cart-email": "^2.104.36",
    "@graphcommerce/magento-cart-items": "^2.105.6",
    "@graphcommerce/magento-cart-payment-method": "^2.107.5",
    "@graphcommerce/magento-cart-pickup": "^2.103.35",
    "@graphcommerce/magento-cart-shipping-address": "^2.104.2",
