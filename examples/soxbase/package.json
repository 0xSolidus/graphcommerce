{
  "name": "@reachdigital/soxbase",
  "homepage": "https://soxbase.vercel.app/",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "next",
    "codegen": "NODE_TLS_REJECT_UNAUTHORIZED=0 CHOKIDAR_USEPOLLING=0 node -r dotenv/config node_modules/.bin/graphql-codegen",
    "dev:codegen": "yarn codegen -w",
    "build:next-config": "yarn workspace @reachdigital/next-config build",
    "build:near-operation": "yarn workspace @reachdigital/graphql-codegen-near-operation-file build",
    "build:relay-optimizer": "yarn workspace @reachdigital/graphql-codegen-relay-optimizer-plugin build",
    "soxbase-api": "yarn workspace @reachdigital/soxbase-api build && yarn start:soxbase-api",
    "build": "yarn build:next-config && yarn build:near-operation && yarn build:relay-optimizer && yarn codegen && next build && yarn next-sitemap",
    "start": "next start",
    "postinstall": "patch-package && patch-typed-document-node"
  },
  "dependencies": {
    "@adobe/apollo-link-mutation-queue": "^1.0.2",
    "@apollo/client": "^3.3.19",
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.4",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/schema-ast": "^1.18.2",
    "@graphql-codegen/typed-document-node": "^1.18.5",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-apollo-client-helpers": "^1.1.5",
    "@graphql-codegen/typescript-document-nodes": "1.17.11",
    "@graphql-codegen/typescript-operations": "1.17.16",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@graphql-typed-document-node/patch-cli": "^3.0.8",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@next/bundle-analyzer": "^10.2.2",
    "@reachdigital/framer-next-pages": "^0.1.0",
    "@reachdigital/graphcms-ui": "^0.1.0",
    "@reachdigital/graphql": "^0.1.0",
    "@reachdigital/graphql-codegen-near-operation-file": "^0.1.0",
    "@reachdigital/graphql-codegen-relay-optimizer-plugin": "^0.1.0",
    "@reachdigital/magento-cart": "^0.1.0",
    "@reachdigital/magento-cart-address": "^0.1.0",
    "@reachdigital/magento-cart-billing-address": "^0.1.0",
    "@reachdigital/magento-cart-checkout": "^0.1.0",
    "@reachdigital/magento-cart-coupon": "^0.1.0",
    "@reachdigital/magento-cart-email": "^0.1.0",
    "@reachdigital/magento-cart-items": "^0.1.0",
    "@reachdigital/magento-cart-payment-method": "^0.1.0",
    "@reachdigital/magento-cart-pickup": "^0.1.0",
    "@reachdigital/magento-cart-shipping-address": "^0.1.0",
    "@reachdigital/magento-cart-shipping-method": "^0.1.0",
    "@reachdigital/magento-category": "^0.1.0",
    "@reachdigital/magento-cms": "^0.1.0",
    "@reachdigital/magento-customer": "^0.1.0",
    "@reachdigital/magento-graphql": "^0.1.0",
    "@reachdigital/magento-payment-braintree": "^0.1.0",
    "@reachdigital/magento-payment-included": "^0.1.0",
    "@reachdigital/magento-payment-klarna": "^0.1.0",
    "@reachdigital/magento-payment-mollie": "^0.1.0",
    "@reachdigital/magento-product": "^0.1.0",
    "@reachdigital/magento-product-bundle": "^0.1.0",
    "@reachdigital/magento-product-configurable": "^0.1.0",
    "@reachdigital/magento-product-downloadable": "^0.1.0",
    "@reachdigital/magento-product-grouped": "^0.1.0",
    "@reachdigital/magento-product-simple": "^0.1.0",
    "@reachdigital/magento-product-virtual": "^0.1.0",
    "@reachdigital/magento-search": "^0.1.0",
    "@reachdigital/magento-store": "^0.1.0",
    "@reachdigital/next-config": "0.1.0",
    "@reachdigital/next-ui": "^0.1.0",
    "@reachdigital/react-hook-form": "^0.1.0",
    "apollo-cache-persist": "^0.1.1",
    "clsx": "^1.1.1",
    "dotenv": "^9.0.2",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.12.4",
    "next": "^10.2.2",
    "next-images": "^1.7.0",
    "next-pwa": "^5.2.21",
    "next-sitemap": "^1.6.57",
    "node-fetch": "^2.6.1",
