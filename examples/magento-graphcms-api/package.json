{
  "name": "@graphcommerce/magento-graphcms-api",
  "homepage": "https://graphcommerce-api.vercel.app/api/graphql",
  "version": "2.105.5",
  "private": true,
  "sideEffects": false,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "next -p 3001",
    "build:next-config": "yarn workspace @graphcommerce/next-config build",
    "build:mesh": "yarn mesh build --require dotenv/config",
    "build": "yarn build:next-config && yarn build:mesh && next build",
    "start": "next start",
    "start-local": "next start -p 3001"
  },
  "dependencies": {
    "@graphcommerce/graphql-mesh": "^3.0.5",
    "@graphcommerce/next-config": "^2.104.4",
    "@graphql-mesh/cli": "^0.41.0",
    "@graphql-mesh/config": "^0.23.8",
    "@graphql-mesh/graphql": "^0.18.16",
    "@graphql-mesh/merger-stitching": "^0.12.7",
    "@graphql-mesh/runtime": "^0.22.0",
    "@graphql-mesh/transform-cache": "^0.9.18",
    "@graphql-mesh/transform-federation": "^0.6.8",
    "@graphql-mesh/transform-filter-schema": "^0.11.6",
    "@graphql-mesh/types": "^0.51.0",
    "dotenv": "^10.0.0",
    "next": "^11.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@graphcommerce/browserslist-config-pwa": "^3.0.1",
    "@graphcommerce/eslint-config-pwa": "^3.0.5",
    "@graphcommerce/prettier-config-pwa": "^3.0.2",
    "@graphcommerce/typescript-config-pwa": "^3.1.0",
    "@playwright/test": "^1.15.0",
    "typescript": "^4.4.3"
  },
  "prettier": "@graphcommerce/prettier-config-pwa",
  "browserslist": [
    "extends @graphcommerce/browserslist-config-pwa"
  ],
  "eslintConfig": {
    "extends": "@graphcommerce/eslint-config-pwa",
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  }
}
