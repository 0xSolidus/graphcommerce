{
  "ci": {
    "collect": {
      "numberOfRuns": 3
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "color-contrast": "off",
        "is-crawlable": "off",
        "tap-targets": "off",
        "mainthread-work-breakdown": "off",
        "max-potential-fid": "off",
        "uses-long-cache-ttl": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "server": {},
    "wizard": {}
  }
}
