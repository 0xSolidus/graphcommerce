{
  "packages": [
    ".",
    "docs",
    "packagesDev/*",
    "packages/*",
    "packages/*/example",
    "packages/*/examples/*",
    "examples/*"
  ],
  "version": "independent",
  "command": {
    "publish": {
      "conventionalCommits": true,
      "ignoreChanges": ["*.md"],
