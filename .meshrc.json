{
  "sources": [
    {
      "name": "GraphCMS",
      "handler": {
        "graphql": {
          "headers": {
            "Authorization": "${GRAPHCMS_BEARER}"
          },
          "endpoint": "${GRAPHCMS}"
        }
      },
      "transforms": [
        {
          "filterSchema": ["Mutation.createContactForm"]
        }
      ]
    },
    {
      "name": "Magento GraphQL",
      "handler": {
        "graphql": {
          "endpoint": "https://develop.pwa-venia.com/graphql"
        }
      }
    }
  ]
}
