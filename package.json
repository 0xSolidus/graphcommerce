{
  "name": "project-reachdigital.nl",
  "homepage": "https://project-reachdigital-nl.now.sh/",
  "version": "5.9.3",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "concurrently yarn:dev-* --kill-others",
    "dev-next": "next dev",
    "dev-codegen": "graphql-codegen -r dotenv/config -w",
    "dev-storybook": "start-storybook --quiet -p 6006",
    "build": "graphql-codegen -r dotenv/config && yarn build-storybook -o public/storybook --quiet && next build",
    "start": "next start",
    "semantic-release": "semantic-release"
  },
  "sideEffects": false,
  "dependencies": {
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^3.1.5",
    "@apollo/react-ssr": "^3.1.5",
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.51",
    "@material-ui/styles": "^4.9.6",
    "@next/bundle-analyzer": "^9.3.4",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.17",
    "apollo-utilities": "^1.3.3",
    "dotenv": "^8.2.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "next": "9.3.5",
    "next-images": "^1.4.0",
    "now": "^18.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^5.6.0",
    "react-schemaorg": "^1.0.0",
    "react-spring": "^9.0.0-canary.809.5.f01ecc2",
    "resize-observer": "^1.0.0",
    "schema-dts": "^0.5.1",
    "sitemap": "^6.1.2",
    "use-resize-observer": "^6.1.0-alpha.3"
  },
  "devDependencies": {
    "@babel/compat-data": "^7.9.0",
    "@babel/core": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@graphql-codegen/cli": "^1.13.3",
    "@graphql-codegen/fragment-matcher": "1.13.3",
    "@graphql-codegen/introspection": "1.13.3",
    "@graphql-codegen/schema-ast": "^1.13.3",
    "@graphql-codegen/typescript": "1.13.3",
    "@graphql-codegen/typescript-document-nodes": "1.13.3",
    "@graphql-codegen/typescript-graphql-files-modules": "1.13.3",
    "@graphql-codegen/typescript-operations": "1.13.3",
    "@graphql-codegen/typescript-react-apollo": "1.13.3",
    "@harelpls/storybook-addon-materialui": "^2.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addons": "^5.3.13",
    "@storybook/core": "^5.3.18",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.18",
    "@storybook/source-loader": "^5.3.17",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/resize-observer-browser": "^0.1.3",
    "@types/use-resize-observer": "^6.0.0",
    "@types/webpack": "^4.41.12",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.30.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "semantic-release": "^17.0.7",
    "semantic-release-slack-bot": "^1.6.1",
    "typescript": "^3.7.5",
    "webpack": "^4.43.0"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "proseWrap": "always",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "printWidth": 80
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ho-nl/project-reachdigital.nl.git"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              {
                "type": "docs",
                "section": "Documentation"
              },
              {
                "type": "feat",
                "section": "Features"
              },
              {
                "type": "fix",
                "section": "Fixes"
              },
              {
                "type": "perf",
                "section": "Performance Improvements"
              },
              {
                "type": "build",
                "section": "Build"
              }
            ]
          }
        }
      ],
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github",
      [
        "semantic-release-slack-bot",
        {
          "notifyOnSuccess": true,
          "notifyOnFail": true,
          "markdownReleaseNotes": true,
          "onSuccessTemplate": {
            "blocks": [
              {
                "type": "section",
                "text": {
                  "type": "mrkdwn",
                  "text": "*New release for $repo_url ðŸŽ‰*\n$release_notes"
                }
              }
            ]
          }
        }
      ]
    ],
    "branches": [
      {
        "name": "master"
      }
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "restricted"
  }
}
