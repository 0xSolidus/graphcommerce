{
  "name": "project-reachdigital.nl",
  "homepage": "https://soxbase.vercel.app",
  "version": "1.43.0",
  "private": true,
  "workspaces": [
    "packagesDev/*",
    "packages/*",
    "examples/*"
  ],
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "postinstall": "patch-package && patch-typed-document-node",
    "dev:api": "yarn pm2 start -s --name api \"NODE_TLS_REJECT_UNAUTHORIZED=0 yarn workspace @reachdigital/soxbase-api dev \" && sleep 5",
    "dev:codegen": "yarn pm2 start -s --name codegen \"CHOKIDAR_USEPOLLING=0 yarn node -r dotenv/config node_modules/.bin/graphql-codegen -w \"",
    "dev:next": "yarn pm2 start -s --name next \"NODE_TLS_REJECT_UNAUTHORIZED=0 yarn workspace @reachdigital/soxbase dev\"",
    "dev:log": "yarn pm2 logs",
    "dev": "yarn stop; yarn dev:api && yarn dev:next && yarn dev:log",
    "postdev": "yarn stop",
    "codegen-run": "NODE_TLS_REJECT_UNAUTHORIZED=0 node -r dotenv/config node_modules/.bin/graphql-codegen",
    "codegen-eslint": "eslint --fix --ignore-path .gitignore '**/*.gql.ts'",
    "codegen-prettier": "yarn prettier --ignore-path .gitignore --write '**/*.gql.ts'",
    "codegen": "yarn build:codegen-run; yarn build:codegen-prettier",
    "build:soxbase-api": "yarn workspace @reachdigital/soxbase-api build && yarn start:soxbase-api",
    "build:soxbase": "yarn workspace @reachdigital/soxbase build",
    "build:lighthouse": "ts-node -O '{\"module\":\"commonjs\"}' --transpile-only lib/lighthouse.ts",
    "build": "yarn stop; yarn build:soxbase-api && yarn build:soxbase && yarn build:lighthouse; yarn stop",
    "stop": "yarn pm2 delete -s all",
    "start:soxbase-api": "yarn pm2 start -s --name api \"NODE_TLS_REJECT_UNAUTHORIZED=0 yarn workspace @reachdigital/soxbase-api start-local\" && sleep 2",
    "start:soxbase": "NODE_TLS_REJECT_UNAUTHORIZED=0 yarn workspace @reachdigital/soxbase start",
    "start": "yarn start:soxbase-api && yarn start:soxbase",
    "eslint:lint": "eslint --ignore-path .gitignore '**/{*.js,*.ts,*.tsx}'",
    "eslint:fix": "eslint --fix --ignore-path .gitignore '**/{*.js,*.ts,*.tsx}'",
    "prettier:fix": "yarn prettier --ignore-path .gitignore --write '**/*.{ts,tsx,css,html}'",
    "tsc:lint": "tsc --noEmit -p ."
  },
  "sideEffects": false,
  "prettier": "@reachdigital/prettier-config-pwa",
  "browserslist": [
    "extends @reachdigital/browserslist-config-pwa"
  ],
  "eslintConfig": {
    "extends": "@reachdigital/eslint-config-pwa",
    "parserOptions": {
      "project": "./tsconfig.json"
    }
  },
  "dependencies": {
    "@adobe/apollo-link-mutation-queue": "^1.0.2",
    "@apollo/client": "^3.2.5",
    "@graphql-codegen/import-types-preset": "^1.18.0",
    "@graphql-mesh/cache-file": "^0.4.17",
    "@graphql-mesh/cli": "^0.11.6",
    "@graphql-mesh/config": "^0.10.12",
    "@graphql-mesh/graphql": "^0.7.5",
    "@graphql-mesh/merger-stitching": "^0.6.16",
    "@graphql-mesh/runtime": "^0.7.11",
    "@graphql-mesh/transform-cache": "^0.7.9",
    "@graphql-mesh/transform-filter-schema": "^0.6.10",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/styles": "^4.10.0",
    "@n1ru4l/graphql-codegen-relay-optimizer-plugin": "^5.0.0-alpha.0",
    "@next/bundle-analyzer": "^10.0.0",
    "@reachdigital/graphql-codegen-relay-optimizer-plugin": "0.1.0",
    "apollo-cache-persist": "^0.1.1",
    "apollo-server-micro": "^2.18.2",
    "clone-deep": "^4.0.1",
    "clsx": "^1.1.1",
    "dotenv": "^8.2.0",
    "framer-motion": "^2.9.1",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "intl": "^1.2.5",
    "next": "10.0.0",
    "next-images": "^1.6.2",
    "next-pwa": "^3.1.5",
    "next-transpile-modules": "^4.1.0",
    "node-fetch": "^2.6.1",
    "patch-package": "^6.2.2",
    "pm2": "^4.5.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-focus-lock": "^2.4.1",
    "react-hook-form": "^6.9.5",
    "react-schemaorg": "^1.2.0",
    "react-spring": "^8.0.27",
    "resize-observer": "^1.0.0",
    "schema-dts": "^0.7.1",
    "sharp": "^0.26.2",
    "sitemap": "^6.3.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "type-fest": "^0.18.0",
    "use-resize-observer": "^6.1.0",
    "vercel": "^20.1.2"
  },
  "devDependencies": {
    "@babel/compat-data": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphql-codegen/add": "^2.0.1",
    "@graphql-codegen/cli": "^1.19.0",
    "@graphql-codegen/fragment-matcher": "2.0.0",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/near-operation-file-preset": "^1.17.11",
    "@graphql-codegen/schema-ast": "^1.17.8",
    "@graphql-codegen/typed-document-node": "^1.17.9",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-document-nodes": "1.17.8",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-typed-document-node/patch-cli": "^3.0.8",
    "@reachdigital/browserslist-config-pwa": "0.1.0",
    "@reachdigital/create-package": "0.1.0",
    "@reachdigital/eslint-config-pwa": "0.1.0",
    "@reachdigital/graphql-codegen-near-operation-file": "0.1.0",
    "@reachdigital/prettier-config-pwa": "0.1.0",
    "@semantic-release/git": "^9.0.0",
    "@types/clone-deep": "^4.0.1",
    "@types/lodash": "^4.14.162",
    "@types/node": "^14.14.5",
    "@types/react": "^16.9.54",
    "@types/resize-observer-browser": "^0.1.4",
    "@types/sharp": "^0.26.0",
    "@types/use-resize-observer": "^6.0.0",
    "@types/webpack": "^4.41.23",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "semantic-release": "^17.2.1",
    "semantic-release-slack-bot": "^1.6.2",
    "typescript": "^4.0.5",
    "webpack": "^5.3.0"
  }
}
